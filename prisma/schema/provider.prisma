model Provider {
  id           Int        @id @default(autoincrement())
  businessName String     @unique
  description  String?
  phoneNumber  String
  ruc          String     @unique
  Purchase     Purchase[]
  products     ProviderProduct[]
  
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

   
}
