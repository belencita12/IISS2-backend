model Vaccine {
  id             Int                      @id @default(autoincrement())
  speciesId      Int  
  name           String
  product        Product? 
  manufacturerId Int
  manufacturer   VaccineManufacturer      @relation(fields: [manufacturerId], references: [id])
  batchId        Int
  batch          VaccineBatch             @relation(fields: [batchId], references: [id])
  cardDetails    VaccinationCardDetails[]

  deletedAt DateTime?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}
