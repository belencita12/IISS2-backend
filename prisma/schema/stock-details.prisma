model StockDetails {
  id         Int      @id @default(autoincrement())
  productId  Int
  product    Product @relation(fields: [productId], references: [id])
  stockId    Int
  stock      Stock @relation(fields: [stockId], references: [id])
  amount     Int

  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?    
}
